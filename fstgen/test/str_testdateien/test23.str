// testing tuples

module test23
imports collection/list/cons collection/tuple/cons
strategies

  main-test23 = 
    test1; 
    test2

  Swap : (x, y) -> (y, x)

  Fst : (x, y) -> x

  Snd : (x, y) -> y

  Split(f, g) = !(<f>, <g>)

  Swap2 = Split(Snd,Fst)

  is-pair = (id,id)

  test1 = 
    !("a", "b");
    Swap;
    is-pair;
    Swap2;
    ?("a", "b")

  test2 =
    !(("b", "c"), "a");
    ?(<Snd>("$", <Swap>), _);
    ?("c", "b")

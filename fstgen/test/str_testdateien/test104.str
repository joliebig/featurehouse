/**
 * Test for issue STR-577: support import of an ATerm from a file
 */
module test104
imports libstratego-lib

signature
  constructors
    Foo : A * () * Tuple([B,C]) -> D
    Bar : List(String) -> A
    Baz : B
    Aap : C

strategies

  main-test104 =
    test1
    ; debug(!"the term: ")
    ; ?Foo(Bar(["a","b"]), None(), (Baz(),Aap()))

  test1 =
    import-term(./test104.trm)

class Complex extends Object {
	int _real;
	int _ima;
	Complex(int real, int ima) {
		super();
		this._real=real;
		this._ima=ima;
	}
	int getReal() {
		return _real;
	}
	int getIma(){
		return _ima;
	}
	Complex add(Complex a) {
		return new Complex(a.getReal()+this.getReal(), this.getIma()+a.getIma());
	}
	Complex mul(Complex a) {
		return new Complex(a.getReal() *this.getReal - this.getIma()*a.getIma(),
			this.getReal * a.getIma() - this.getIma()*a.getReal());
	}
}
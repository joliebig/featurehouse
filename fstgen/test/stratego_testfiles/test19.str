module test19
imports
   libstratego-lib

signature
constructors
f: a
f : a -> a
aap: a

strategies
   main-test19 = string-test
   
   string-test =
     test-suite(!"string-test",
        explode1;
        explode2;
        explode3;
        implode1;
        implode2;
        implode3
     )
   
   explode1 =
     apply-test(!"explode1",
       explode-term,
       !f(),
       !("f", [])
       )
      
   explode2 =
     apply-test(!"explode2",
       explode-term,
       !"f",
       !("\"f\"", [])
       )
      

   explode3 =
     apply-test(!"explode3",
       explode-term,
       !"\"f\"",
       !("\"\\\"f\\\"\"", [])
       )
      
      
   implode1 =
     apply-test(!"implode1",
       mkterm,
       !("f",[]),
       !f()
       )
      
   implode2 =
     apply-test(!"implode2",
       mkterm,
       !("\"f\"", []),
       !"f"
       )
      

   implode3 =
     apply-test(!"implode3",
       mkterm,
       !("\"\\\"f\\\"\"", []),
       !"\"f\""
       )
      
      

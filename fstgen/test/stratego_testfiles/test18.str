module test18
imports libstratego-lib
signature
  constructors
    A : Term
    B : Term
    F : Term * Term -> Term
    g : Term
    g: a -> Term
strategies

  main-test18 = 
    test-suite(!"test18",
	test0;
	test1;
	test2;
	test3;
//	test4;
	test5;
	test6
    )

  test0 =
    apply-test(!"test0a"
	, [explode-term]
	, !["f"]
	, ![("\"f\"", [])]
	);
    apply-test(!"test0b"
	, explode-term
	, !F(A,B)
	, !("F", [A, B])
	)

  test1 =
    apply-test(!"test1a"
	, \ [f#(args)] -> [(f, args)] \
	, !["f"]
	, ![("\"f\"", [])]
	);
    apply-test(!"test1b"
	, \ f#(args) -> (f, args) \
	, !F(A,B)
	, !("F", [A, B])
	)

 test2 =
    apply-test(!"test2a"
	, \ [f#(args)] -> [f#(args)] \
	, !["f"]
	, !["f"]
	);
    apply-test(!"test2b"
	, \ [(f,args)] -> [f#(args)] \
	, ![("g",[])]
	, ![g]
	);
    apply-test(!"test2c"
	, \ [(f,args)] -> [f#(args)] \
	, ![("\"f\"",[])]
	, !["f"]
	)

 test3 =
    apply-test(!"test3a"
	, \ [f#(args)] -> [f#(args)] \
	, ![F(A,B)]
	, ![F(A,B)]
	);
    apply-test(!"test3b"
	, explode-term
	, !F(A,B)
	, !("F", [A, B])
	);
    apply-test(!"test3c"
	, mkterm
	, !("F", [A, B])
	, !F(A,B)
	)

/*
  test4 =
    apply-test(!"test4a"
	, [explode-term]
	, ![3.0]
	, ![(3.0, [])]
	);
    apply-test(!"test4b"
	, explode-term
	, !3.0
	, !(3.0, [])
	)
*/

  test5 =
    apply-test(!"test5a"
	, explode-term
	, !["a"]
	, !([], ["a"])
	);
    apply-test(!"test5b"
	, explode-term
	, ![]
	, !([], [])
	)

  test6 =
    apply-test(!"test6a"
	, mkterm
	, !([], ["a"])
	, !["a"]
	);
    apply-test(!"test6b"
	, mkterm
	, !([], [])
	, ![]
	)
